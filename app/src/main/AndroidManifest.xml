<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.stock_control">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.activity.PurchaseOrderActivity"/>
        <activity android:name=".ui.activity.TransactionActivity"/>
        <activity android:name=".ui.activity.EditorActivity"/>
        <activity android:name=".ui.activity.ProductAddActivity"/>
        <activity android:name=".ui.activity.ProductDeleteActivity"/>
        <activity android:name=".ui.activity.ProductEditorActivity"/>
        <activity android:name=".ui.activity.SupplierAddActivity"/>
        <activity android:name=".ui.activity.SupplierDeleteActivity"/>
        <activity android:name=".ui.activity.SupplierEditorActivity"/>
    </application>

</manifest>